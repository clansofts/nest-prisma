    "build": "rimraf dist && tsc",
    "prisma": "./node_modules/.bin/prisma",
    "lint": "./node_modules/.bin/tslint -p ."
  },
  "dependencies": {
    "@nestjs/common": "4.6.3",
    "@nestjs/core": "4.6.3",
    "@nestjs/graphql": "2.0.0",
    "@nestjs/microservices": "4.6.1",
    "@nestjs/testing": "4.6.1",
    "@nestjs/typeorm": "2.0.0",
    "@nestjs/websockets": "4.5.8",
    "bcryptjs": "2.4.3",
    "graphql": "0.13.1",
    "graphql-yoga": "1.3.2",
    "jsonwebtoken": "8.1.1",
    "mysql": "2.15.0",
    "prisma": "1.2.6",
    "prisma-binding": "1.5.12",
    "reflect-metadata": "0.1.12",
    "typeorm": "0.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/node": "9.4.6",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.14.1",
    "nodemon": "1.15.0",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
